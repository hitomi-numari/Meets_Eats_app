<table class="table table-hover">
  <thead>
    <h2 class="mb-5">メッセージ一覧</h2>
  </thead>
  <tbody>
    <% @rooms.each do |room| %>
    <tr>
      <% if room.target_user(current_user).present? %>
      <td width="10%">
        <%= link_to image_tag(room.target_user(current_user).profile.icon_url), room_messages_path(room) %>
      </td>
      <td width="90%">
        <%= room.target_user(current_user).profile.nickname%>
      </td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
